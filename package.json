{
  "name": "advide",
  "version": "0.0.16",
  "description": "",
  "main": "src/ide/advide-main.js",
  "scripts": {
    "build:game": "esbuild src/advgame.jsx --jsx-factory=h --jsx-fragment=Fragment --bundle --outfile=res/advgame.js --inject:src/utils/preact-shim.js --loader:.png=dataurl --loader:.svg=dataurl --target=es2017",
    "build:ide": "esbuild src/ide/advide.jsx --external:electron --jsx-factory=h --jsx-fragment=Fragment --bundle --outfile=res/advide.bundle.js --inject:src/utils/preact-shim.js --loader:.png=dataurl --loader:.svg=dataurl --target=es2017",
    "build:admin": "esbuild src/admin/advadmin.jsx --jsx-factory=h --jsx-fragment=Fragment --bundle --outfile=res/advadmin.bundle.js --inject:src/utils/preact-shim.js --loader:.png=dataurl --loader:.svg=dataurl --target=es2017 --minify",
    "build:ed": "esbuild src/ed/adved.jsx --jsx-factory=h --jsx-fragment=Fragment --bundle --outfile=res/adved.bundle.js --inject:src/utils/preact-shim.js --loader:.png=dataurl --loader:.svg=dataurl --target=es2017",
    "build": "npm run build:game && npm run build:ide && npm run build:admin && npm run build:ed",
    "dev": "esbuild src/advgame.jsx --jsx-factory=h --jsx-fragment=Fragment --bundle --outfile=res/advgame.js --inject:src/utils/preact-shim.js --loader:.png=dataurl --loader:.svg=dataurl --target=es2017 --servedir=res",
    "dev:ed": "esbuild src/ed/adved.jsx --jsx-factory=h --jsx-fragment=Fragment --bundle --outfile=res/adved.bundle.js --inject:src/utils/preact-shim.js --loader:.png=dataurl --loader:.svg=dataurl --target=es2017 --servedir=res",
    "test": "jasmine",
    "start": "npm run build && electron .",
    "postinstall": "link-module-alias"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ace-builds": "^1.4.12",
    "electron": "^15.3.0",
    "esbuild": "^0.12.17",
    "jasmine": "^3.9.0",
    "link-module-alias": "^1.2.0",
    "module-alias": "^2.2.2",
    "preact": "^10.5.14",
    "react-ace": "^9.5.0",
    "react-split": "^2.0.13"
  },
  "dependencies": {
    "electron-prompt": "^1.7.0",
    "events": "^3.3.0",
    "yaml": "^1.10.2"
  },
  "_moduleAliases": {
    "react": "node_modules/preact/compat"
  }
}
